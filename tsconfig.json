{
  "exclude": [
    "res/**/*",
    "node_modules/**/*",
    "dist/**/*",
    "docs/**/*",
    ".parcel-cache/**/*",
    "res"
  ],
  "compilerOptions": {
    "plugins": [
      {
        "name": "ts-lit-plugin"
      },
      {
        "name": "typescript-lit-html-plugin"
      }
    ],
    "target": "ES2020",
    "experimentalDecorators": true,
    "module": "commonjs",
    "sourceMap": true,
    "sourceRoot": "./src",
    "outDir": "docs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}